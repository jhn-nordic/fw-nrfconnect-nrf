menu "nRFCloud A-GPS module settings"

config NRF_CLOUD_AGPS_MODULE_ENABLE
	bool "Enable nRF Cloud A-GPS module"
	depends on NRF9160_GPS
	depends on MODEM_INFO
	depends on MODEM_INFO_ADD_NETWORK
	help
	  Enable the the nRF Cloud A-GPS module

if NRF_CLOUD_AGPS_MODULE_ENABLE

module = NRF_CLOUD_AGPS_MODULE
module-str = nRFcloud a-gps module
source "subsys/logging/Kconfig.template.log_config"

endif
endmenu
